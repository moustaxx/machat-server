{
    "name": "machat-server",
    "author": "moustaxx",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "node ./dist",
        "clean": "rm -rf dist",
        "build": "yarn clean && yarn generate && tsc",
        "generate": "yarn generate:prisma",
        "generate:prisma": "prisma generate",
        "postinstall": "yarn generate",
        "test": "jest",
        "lint": "eslint --ignore-path .gitignore --cache --ext .ts,.tsx ./src",
        "dev": "tsnd --respawn --transpile-only --ignore-watch ./src/generated --cache-directory ./.yarn/.cache/tsnd ./src/start",
        "debug": "tsnd --inspect --respawn --transpile-only --ignore-watch ./src/generated --cache-directory ./.yarn/.cache/tsnd ./src/start"
    },
    "resolutions": {
        "eslint-config-airbnb-typescript/@typescript-eslint/parser": "^4.15.1",
        "eslint-config-airbnb-typescript/eslint-config-airbnb": "*",
        "eslint-config-airbnb-typescript/eslint-config-airbnb-base": "*"
    },
    "dependencies": {
        "@devoxa/prisma-relay-cursor-connection": "^1.1.1",
        "@prisma/client": "~2.19.0",
        "apollo-server-errors": "^2.5.0",
        "argon2": "^0.27.2",
        "class-validator": "^0.13.1",
        "connect-pg-simple": "^6.2.1",
        "dotenv": "^8.2.0",
        "fastify": "^3.14.2",
        "fastify-cookie": "^5.3.0",
        "fastify-cors": "^5.2.0",
        "fastify-session": "patch:fastify-session@^5.2.1#./.yarn/patches/fastify-session_fix-FastifyDeprecation.diff",
        "graphql": "^15.5.0",
        "graphql-fields": "^2.0.3",
        "graphql-scalars": "^1.9.0",
        "mercurius": "^7.4.0",
        "prisma-machat": "link:./prisma/client",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1",
        "typegraphql-prisma": "^0.12.0"
    },
    "devDependencies": {
        "@types/connect-pg-simple": "^4.2.2",
        "@types/eslint": "^7.2.10",
        "@types/graphql-fields": "^1.3.3",
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.39",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.24.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.3.5",
        "jest": "^26.6.3",
        "jest-environment-node": "^26.6.2",
        "light-my-request": "^4.4.1",
        "pg": "^8.6.0",
        "prisma": "~2.19.0",
        "ts-jest": "^26.5.4",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.2.4"
    }
}
